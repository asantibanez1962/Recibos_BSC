      PROCEDURE MyWindow( ubicacion is string)
      gsubicacion is string = ubicacion 
      MyMarker is Marker
      MyPosition is geoPosition
      gsLongitud is string
      gsLatitud is string
      nuevaubicacion= false
      
      gsLongitud = ExtractString(gsubicacion, 1, ",")
      gsLatitud = ExtractString(gsubicacion, 2, ",")
      
      MyPosition..Latitude = gsLongitud
      MyPosition..Longitude = gsLatitud
      MyMarker..Position = MyPosition
      //MyMarker..ActionClick = ProcMarkerClick
      
      MapAddMarker(MAP_Control,MyMarker)
      MapDisplayPosition(MAP_Control,MyPosition)
      
