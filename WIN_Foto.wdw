         bufImageBuffer is buffer 
         
         <if ExecutionTarget=Android>
         		IF InAndroidEmulatorMode() OR InAndroidSimulatorMode() THEN
         			sPhoto="c:\sistemas\ws\productor_cedula_01-01052.jpg"
         			sdest="c:\sistemas\ws\productor_cedula_01-01052a.jpg"
         			fdelete(sDest)
         	//		MyImage=dLoadImage(sphoto)
         		ELSE
         			sPhoto = PhotoRunApp()
         			IF sPhoto = "" THEN
         				info("No se cargÃ³ foto")
         				return
         			else
         				//info(sPhoto)
         			END
         		END
         		IMG_FotoTemp..Width = -1
         		IMG_FotoTemp..Height = -1
         		IMG_FotoTemp=sPhoto
         		// Get the Width and Heigth of the image
         		ImageInfo = BitmapInfo(sPhoto)
         		nPich = Val(ExtractString(ImageInfo, 3))
         		nPicw =  Val(ExtractString(ImageInfo, 2))
         		
         		if nPich>1152 or nPicw>2048 then
         			IMG_FotoTemp..Width = 2048
         			IMG_FotoTemp..Height = 1152
         			sDest = fExtractPath(sPhoto, fDrive + fDirectory + fFileName) + "_thumb.jpg"
         			bufImageBuffer=dSaveImageJPEG(IMG_FotoTemp,sDest)
         			//Info("grande",sDest)
         		else
         			sDest=sPhoto
         			//info("normal")
         		end
         		IMG_FotoTemp=sDest
         		//if InAndroidEmulatorMode() or InAndroidSimulatorMode() then
         		//	sDest=sPhoto
         		//else
         		//	VideoGenerateThumbnail(sPhoto,sDest)
         		//	info(sDest)
         		//END
         	//	MyImage=dLoadImage(sDest)
         	//	HLinkMemo(proveedores,foto,sDest)
         		IMG_Foto=sDest
         	//ScreenToFile()
         	//hsave(proveedores)
         	//HModify(proveedores)
         	
         		fDelete(sPhoto)
         	//	fdelete(sDest)	no se puede borrar hasta despues de salvar
         	
         		GR_campos..Modified=true
         	
         <else if ExecutionTarget=iOS>
         	IF proveedores.foto="" THEN
         		PhotoRunApp(CB_PhotoRunApp)
         			INTERNAL PROCEDURE CB_PhotoRunApp(sPath is string)
         			// If the return is an empty string vide (""), an error occurred
         			IF sPath ~= "" THEN
         				Error(ErrorInfo())
         				RETURN
         			END
         			MyImage = dLoadImage(sPath)
         			proveedores.m_esnuevo=True
         			//Info("Foto grabada en : " + sPath)
         			proveedores.foto=MyImage
         			proveedores.m_esmodificado=True
         			ScreenToFile()
         			HSave(proveedores)
         			END
         	END
         	
         <end>
         
         
